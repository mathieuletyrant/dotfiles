{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#12141a",
    "black": "#12141a",
    "blue": "#78dce8",
    "brightBlack": "#404658",
    "brightBlue": "#61eeff",
    "brightCyan": "#61ffcf",
    "brightGreen": "#a9f65c",
    "brightMagenta": "#fd7df4",
    "brightRed": "#ff6764",
    "brightWhite": "#ffffff",
    "brightYellow": "#ffd866",
    "cyan": "#78e8c6",
    "foreground": "#a9aec1",
    "green": "#a9dc76",
    "magenta": "#e991e3",
    "orange": "#fc9867",
    "purple": "#ab9df2",
    "red": "#fc6a67",
    "white": "#c6cad7",
    "yellow": "#ffd866"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": "\uf120 {{ .Name }} ",
          "foreground": "p:black",
          "background": "p:blue",
          "type": "shell"
        },
        {
          "style": "diamond",
          "template": "<parentBackground>\ue0b0</> \uf292 ",
          "foreground": "p:black",
          "background": "p:red",
          "type": "root"
        },
        {
          "properties": {
            "folder_icon": " \uf07b ",
            "home_icon": "\ue617",
            "style": "folder"
          },
          "style": "powerline",
          "template": " \uf07b\uea9c {{ .Path }} ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "background": "p:orange",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "background": "p:yellow",
          "type": "git"
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " \ueba2 {{ .FormattedMs }}\u2800",
          "foreground": "p:black",
          "background": "p:purple",
          "type": "executiontime"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <p:red>\ue5fa</> ",
            "yarn_icon": " <p:blue>\ue6a7</>"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "p:green",
          "background": "p:black",
          "type": "node"
        },
        {
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "p:blue",
          "background": "p:white",
          "type": "go"
        },
        {
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "p:red",
          "background": "p:white",
          "type": "angular"
        },
        {
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "p:orange",
          "background": "p:black",
          "type": "aws"
        },
        {
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "foreground": "p:white",
          "background": "p:blue",
          "type": "kubectl"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "foreground": "p:blue",
          "background": "",
          "type": "text"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285\ueab6 ",
          "foreground": "#e0f8ff",
          "background": "",
          "type": "status",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "auto_upgrade": true
}
